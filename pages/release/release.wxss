.release-box{
  width: 100%;
  height:90%;
  background-color: #fff;
  position: relative;
  padding: 0 20rpx;
}
.release-text{
  width: 90%;
  height: 90%;
  
}
.release-footer{
  position: absolute;
  bottom: 0;
  left:0;
  height:90rpx;
  width: 100%;
 
  display: flex;
  vertical-align: middle;
}
.release-footer-left{
  flex:1;
  height:100%;
  margin-left: 46rpx;
}
.icon-biaoqing-copy{
  font-size: 60rpx;
  color:#000;
  height: 100%;
  line-height: 90rpx;
}
.release-footer-right{
  width: 208rpx;
  height: 100%;
  display: flex;
  vertical-align: middle;
  line-height: 90rpx;
}
.release-num{
  color: #ccc;
}
.release-btn{
  color: #000;
  font-size: 30rpx;
  margin-left: 10rpx;
  width: 130rpx;
  background-color: #ccc;
  border-radius: 50rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  margin-top: 20rpx;
}
